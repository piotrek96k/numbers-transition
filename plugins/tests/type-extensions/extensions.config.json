{
  "tsConfig": "tsconfig.plugins.test.json",
  "extensionsFilePath": "plugins/tests/type-extensions/extensions/extensions.ts",
  "extensions": {
    "Predicate": { "type": "Boolean", "typeCheck": "typeof VALUE === 'boolean' || VALUE instanceof Boolean" },
    "Double": { "type": "Number", "typeCheck": "typeof VALUE === 'number' || VALUE instanceof Number" },
    "Long": { "type": "BigInt", "typeCheck": "typeof VALUE === 'bigint'" },
    "CharSequence": { "type": "String", "typeCheck": "typeof VALUE === 'string' || VALUE instanceof String" },
    "Pattern": { "type": "RegExp", "typeCheck": "VALUE instanceof RegExp" },
    "List": { "type": "Array", "typeCheck": "Array.isArray(VALUE)" },
    "Struct": { "type": "Object", "typeCheck": "(typeof VALUE === 'object' || typeof VALUE === 'function') && VALUE !== null" }
  }
}

{
  "name": "numbers-transition",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/numbers-transition.cjs",
  "module": "./dist/numbers-transition.js",
  "types": "./dist/numbers-transition.d.ts",
  "exports": {
    ".": {
      "types": "./dist/numbers-transition.d.ts",
      "import": "./dist/numbers-transition.js",
      "require": "./dist/numbers-transition.cjs"
    }
  },
  "scripts": {
    "prepare": "ts-patch install -s && husky",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build:plugins": "tsc -p tsconfig.plugins.json && vite build -c vite.plugins.ts",
    "test:plugins": "npm run build:plugins && tsc -p tsconfig.plugins.test.json && vitest -c vite.plugins.test.ts run",
    "storybook": "npm run build:plugins && storybook dev -p 6006",
    "build": "npm run test:plugins && tsc && vite build",
    "build:storybook": "npm run build && storybook build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "styled-components": ">=6.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@storybook/addon-docs": "^10.2.7",
    "@storybook/addon-onboarding": "^10.2.7",
    "@storybook/react-vite": "^10.2.7",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-storybook": "^10.2.7",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "storybook": "^10.2.7",
    "terser": "^5.46.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18"
  }
}

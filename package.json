{
  "name": "numbers-transition",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/numbers-transition.cjs",
  "module": "./dist/numbers-transition.js",
  "types": "./dist/numbers-transition.d.ts",
  "exports": {
    ".": {
      "types": "./dist/numbers-transition.d.ts",
      "import": "./dist/numbers-transition.js",
      "require": "./dist/numbers-transition.cjs"
    }
  },
  "scripts": {
    "prepare": "ts-patch install -s && husky",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build:plugins": "tsc -p tsconfig.plugins.json",
    "build": "npm run build:plugins && tsc && vite build",
    "build:storybook": "npm run build && storybook build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "styled-components": ">=6.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@storybook/addon-docs": "^10.0.0",
    "@storybook/addon-onboarding": "^10.0.0",
    "@storybook/react-vite": "^10.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.0.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "storybook": "^10.0.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4"
  }
